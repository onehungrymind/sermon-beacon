{
  "version": 1,
  "projects": {
    "user-portal": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/user-portal",
      "sourceRoot": "apps/user-portal/src",
      "prefix": "sb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/user-portal",
            "index": "apps/user-portal/src/index.html",
            "main": "apps/user-portal/src/main.ts",
            "polyfills": "apps/user-portal/src/polyfills.ts",
            "tsConfig": "apps/user-portal/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/user-portal/src/favicon.ico",
              "apps/user-portal/src/assets"
            ],
            "styles": [
              "apps/user-portal/src/styles.scss",
              "libs/theme/theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/user-portal/src/environments/environment.ts",
                  "with": "apps/user-portal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "user-portal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "user-portal:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "user-portal:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/user-portal/tsconfig.app.json",
              "apps/user-portal/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/user-portal/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/user-portal/jest.config.js",
            "tsConfig": "apps/user-portal/tsconfig.spec.json",
            "setupFile": "apps/user-portal/src/test-setup.ts"
          }
        }
      }
    },
    "user-portal-e2e": {
      "root": "apps/user-portal-e2e",
      "sourceRoot": "apps/user-portal-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/user-portal-e2e/cypress.json",
            "tsConfig": "apps/user-portal-e2e/tsconfig.e2e.json",
            "devServerTarget": "user-portal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "user-portal:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/user-portal-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/user-portal-e2e/**"]
          }
        }
      }
    },
